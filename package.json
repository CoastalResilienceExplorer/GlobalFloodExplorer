    "@types/google.maps": "^3.54.10",
    "@types/jest": "^29.5.3",
    "@types/mapbox-gl": "^3.1.0",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.27",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.16",
    "cypress": "^13.4.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.3.0",
    "postcss": "^8.4.32",
    "prettier": "^3.0.1",
    "serve": "^14.2.1",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.5.4",
    "wait-on": "^7.0.1"
  },
  "eslintConfig": {
